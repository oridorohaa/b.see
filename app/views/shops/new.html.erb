<div data-controller='address-input'>
<%= simple_form_for(@shop) do |f| %>
<%= f.input :address  %>
<%= f.input :latitude%>
<%= f.input :longitude %>
<%= f.input :hours %>
<%= f.input :phone_number %>
<%= f.input :description %>
<%= f.input :website %>
<%= f.input :photos, as: :file, input_html: { multiple: true } %>
<%= f.submit "add shop" %>
<% end %>
</div>

